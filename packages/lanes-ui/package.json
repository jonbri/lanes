{
  "name": "lanes-ui",
  "version": "0.2.0",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "scripts": {
    "lint": "TIMING=1 eslint \"**/*.ts*\"",
    "build": "tsup index.ts --format cjs --dts",
    "dev": "yarn build -- --watch",
    "publish:start": "cd packages/lanes-ui && yarn changeset",
    "publish:version": "cd packages/lanes-ui && yarn changeset version",
    "publish": "cd packages/lanes-ui && yarn changeset publish"
  },
  "devDependencies": {
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "eslint": "^7.32.0",
    "eslint-config-custom": "*",
    "tsconfig": "*",
    "tsup": "^6.6.0",
    "typescript": "^4.5.2"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
    "@changesets/cli": "^2.26.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
